# PostgreSQL connection (used by connector)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=health_tables
DB_CHANNEL=fhir_outbox_event

# Worker configuration
WORKER_ID=worker-1
MAX_RETRIES=5
POLL_INTERVAL_MS=2000

# FHIR server (target)
FHIR_SERVER=https://fhir-bootcamp.medblocks.com/fhir/

# Metrics
METRICS_PORT=9464
METRICS_POLL_SECONDS=10

# Processing limits
PROCESS_BURST_LIMIT=20
